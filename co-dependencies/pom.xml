        <java.version>1.8</java.version>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <maven.plugin.version>3.1</maven.plugin.version>
        <maven.compile.source>1.8</maven.compile.source>
        <maven.compile.target>1.8</maven.compile.target>
        <spring.boot.web.version>2.7.12</spring.boot.web.version>
        <lombok.version>1.18.22</lombok.version>
        <swagger.ui.version>2.9.2</swagger.ui.version>
        <swagger.version>2.9.2</swagger.version>
        <spring.boot.autoconfigure.version>2.7.12</spring.boot.autoconfigure.version>
        <spring.boot.redis.version>2.7.12</spring.boot.redis.version>
        <commons.pool.version>2.11.1</commons.pool.version>
        <jackson.annotations.version>2.14.2</jackson.annotations.version>
        <jackson.databind.version>2.14.2</jackson.databind.version>
